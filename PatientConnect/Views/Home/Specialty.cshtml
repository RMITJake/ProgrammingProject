@{
    String specialty = "Internal Medicine";
    String[] doctrine = {"Cardiology", "Gastronenterology", "Edocrinology", "Pulmonology"};
    String[] images = {"Doctor.png"};
}
<link rel="stylesheet" href="~/css/card.css" asp-append-version="true" />

<section1><- @specialty</section1>

<section2>
    <top>top</top>
    <quadrants>
        <q1>Q1</q1>
        <q2>Q2</q2>
        <q3>Q3</q3>
        <q4>Q4</q4>
    </quadrants>
</section2>

<section3>
    @for(int i=0; i<12; i++){
        <chatnowcard>
            <profilephoto>photo</profilephoto>
            <profiledescription>
                <h3>Dr Card @i</h3>
                <p>Dr's specialty</p>
                <p>X years | 100% rating</p>
            </profiledescription>
            <chatbutton><a class="chatbutton">Chat</a></chatbutton>
        </chatnowcard>
    }
</section3>